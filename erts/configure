echo "$as_me:$LINENO: result: $DED_LDFLAGS" >&5
echo "${ECHO_T}$DED_LDFLAGS" >&6
echo "$as_me:$LINENO: checking for 'runtime library path' linker flag" >&5
echo $ECHO_N "checking for 'runtime library path' linker flag... $ECHO_C" >&6
if test "x$DED_LD_FLAG_RUNTIME_LIBRARY_PATH" != "x"; then
